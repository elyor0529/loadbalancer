<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>netcoreapp2.0;net461</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>3.1.0</Version>
    <Authors>Joel Christner</Authors>
    <Company>Joel Christner</Company>
    <Product>Kvpbase LoadBalancer</Product>
    <Description>HTTP loadbalancer in C#</Description>
    <Copyright>(c)2019 Joel Christner</Copyright>
    <PackageProjectUrl>https://github.com/kvpbase/loadbalancer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kvpbase/loadbalancer</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>loadbalancer</PackageTags>
    <PackageReleaseNotes>Refactor, retarget to .NET Core and .NET Framework</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestWrapper" Version="1.0.9" />
    <PackageReference Include="SyslogLogging" Version="1.0.10" />
    <PackageReference Include="Watson" Version="1.6.0" />
  </ItemGroup>

</Project>
